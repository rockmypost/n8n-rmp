# Core
N8N_HOST=your-domain.example.com
N8N_PROTOCOL=https
WEBHOOK_URL=https://your-domain.example.com
VUE_APP_URL=https://your-domain.example.com
TIMEZONE=UTC

# User management
# Set to "true" to disable multi-user features if desired
N8N_USER_MANAGEMENT_DISABLED=true
N8N_OWNER_EMAIL=owner@example.com
# REQUIRED: set a strong password in your private .env (do not commit real values)
N8N_OWNER_PASSWORD=__SET_STRONG_PASSWORD__

# Basic auth for the instance (optional)
N8N_BASIC_AUTH_ACTIVE=false
# N8N_BASIC_AUTH_USER=__SET_ME__
# N8N_BASIC_AUTH_PASSWORD=__SET_ME__

# OAuth (optional)
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=

# Performance / logs
N8N_PAYLOAD_SIZE_MAX=16
N8N_METRICS=false
N8N_LOG_LEVEL=info
N8N_PERSISTED_BINARY_DATA_TTL=24

# SSL / Let's Encrypt
LETSENCRYPT_EMAIL=letsencrypt@example.com
